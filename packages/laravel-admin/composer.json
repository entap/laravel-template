{
    "name": "entap/laravel-admin",
    "description": "Laravel Admin",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ii masamitsu",
            "email": "ii@entap.co.jp"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "laravel/ui": "^3.1",
        "laravel/framework": "^8.0",
        "kalnoy/nestedset": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Entap\\Admin\\": "src/",
            "Database\\Seeders\\Entap\\Admin\\": "database/seeders",
            "Database\\Factories\\Entap\\Admin\\Database\\Models\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Entap\\Admin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Entap\\Admin\\Facades\\Admin"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.4"
    },
    "scripts": {
        "test": "phpunit"
    }
}
