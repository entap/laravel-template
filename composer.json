{
    "name": "entap/laravel-template",
    "type": "project",
    "description": "Entap Laravel Template.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "entap/laravel-admin": "^0.1",
        "entap/laravel-auth": "dev-main",
        "entap/laravel-client-packager": ">=0.0.5",
        "entap/laravel-log-viewer": "dev-main",
        "entap/laravel-oauth-firebase": "dev-main",
        "entap/laravel-oauth-line": "dev-main",
        "entap/laravel-request-logger": "^0.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "kreait/firebase-php": "^5.14",
        "kreait/laravel-firebase": "^3.0",
        "laravel/framework": "^8.12",
        "laravel/passport": "^10.1",
        "laravel/tinker": "^2.5"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "php artisan test",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/entap/laravel-auth"
        },
        {
            "type": "vcs",
            "url": "https://github.com/entap/laravel-oauth-firebase"
        },
        {
            "type": "vcs",
            "url": "https://github.com/entap/laravel-oauth-line"
        },
        {
            "type": "vcs",
            "url": "https://github.com/entap/laravel-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/entap/laravel-log-viewer"
        },
        {
            "type": "vcs",
            "url": "https://github.com/entap/laravel-client-packager"
        },
        {
            "type": "vcs",
            "url": "https://github.com/entap/laravel-request-logger"
        }
    ]
}
